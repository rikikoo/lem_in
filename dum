ants=9
plen=4
llen=5
np=1

1. diff    = llen-plen       = 5-4     = 1
2. pants   = diff>ants ? ants : diff   = 1
3. tants   = (ants-pants)/np = (9-1)/1 = 8
4. cost    = plen+tants      = 4+5     = 9
5. maxcost = max(cost)       = 13      = 13


